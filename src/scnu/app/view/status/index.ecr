<table class="table table-hover table-responsive datatable" data-source="/status">
  <thead>
    <tr>
      <th></th>
      <th>#</th>
      <th><%= t(Username) %></th>
      <th><%= t(Title) %></th>
      <th><%= t(Result) %></th>
      <th><%= t(TimeCost) %></th>
      <th><%= t(SpaceCost) %></th>
      <th><%= t(Language) %></th>
      <th><%= t(SubmitTime) %></th>
    </tr>
  </thead>
</table>
<div class="searchable" value="<%= t(Username) %> / <%= t(Title) %> <%= t(Language) %>"></div>
