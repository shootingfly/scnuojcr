<!doctype html>
<html>
  <head>
    <title><%= Sidekiq::NAME %></title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1.0" />
    <link href="<%= x.root_path %>stylesheets/bootstrap.css" media="screen" rel="stylesheet" type="text/css" />
    <link href="<%= x.root_path %>stylesheets/application.css" media="screen" rel="stylesheet" type="text/css" />
    <link rel="shortcut icon" type="image/ico" href="<%= root_path %>images/favicon.ico" />
    <script type="text/javascript" src="<%= x.root_path %>javascripts/application.js"></script>
    <script type="text/javascript" src="<%= x.root_path %>javascripts/locales/jquery.timeago.<%= x.locale %>.js"></script>
    <meta name="google" content="notranslate" />
  </head>
  <body class="admin">
    <%= ecr "_nav" %>
    <div id="page">
      <div class="container">
        <div class="row">
          <div class="col-sm-12 summary_bar">
            <%= ecr "_summary" %>
          </div>

          <div class="col-sm-12">
            <%= content %>
          </div>
        </div>
      </div>
    </div>
    <%=  ecr "_footer" %>
    <%= ecr "_poll_js" %>
  </body>
</html>
